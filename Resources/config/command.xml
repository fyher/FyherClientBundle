<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
       
        <service id="fyher_client.install.param" class="Fyher\ClientBundle\Command\InstallCommand">
            <tag name="console.command" command="fyher_client:install:param" />
        </service>
        <service id="fyher_client.rgpd.check" class="Fyher\ClientBundle\Command\RgpdCommand">
            <tag name="console.command" command="fyher_client:rgpd:check" />
        </service>
        <service id="fyher_client.alarme.start" class="Fyher\ClientBundle\Command\AlarmeCommand">
            <tag name="console.command" command="fyher_client:alarme:start" />
        </service>

    </services>

</container>
